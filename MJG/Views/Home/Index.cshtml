@model MJG.Models.IEClass.Class1
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="google-site-verification" content="X6k4CYxLvJ5EP5sDl6IZiQoxGnc1x_0jOKZ_-q1qytY" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Mert Güney Software Engineer Kişisel Web Sitesi, Yazılım Geliştirme, Web Sitesi Geliştirme, Masaüstü Uygulama Geliştme">
    <meta name="author" content="Mert Guney">
    <meta name="keywords" content="Mert,Güney,Mert Güney,Software Engineer,HTML,CSS,JAVA,C#,MVC5,Javascript">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <title>Mert Güney</title>

    <!-- STYLES -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:200i,300,300i,400,400i,500,500i,600,700,700i,800&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <link href="~/mjg/css/plugins.css" rel="stylesheet" />
    <link href="~/mjg/css/stylem.css" rel="stylesheet" />
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-172492605-1');
    </script>

</head>

<body>

    <!-- PRELOADER -->
    <div class="arlo_tm_preloader">
        <div class="spinner_wrap">
            <div class="spinner"></div>
        </div>
    </div>
    <!-- /PRELOADER -->
    <!-- WRAPPER ALL -->
    <div class="arlo_tm_all_wrap">
        <div class="wrapper_inner">
            <div class="arlo_tm_popup_news scrollable">
                <a class="closer" href="#">Close</a>
                <div class="container">
                    <div class="news_inner"></div>
                </div>
            </div>

            <!-- Topbar -->
            <div class="arlo_tm_topbar">
                <div class="topbar_inner">
                    <div class="container">
                        <div class="topbar_in">
                            <div class="logo">
                                <a href="#"><img src="~/mjg/img/logo/mobile.png" alt="" /></a>
                            </div>
                            <div class="trigger">
                                <a href="#">
                                    <span class="menu">Menu</span>
                                    <span class="close">Close</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="dropdown">
                    <div class="container">
                        <div class="dropdown_inner">
                            <ul>
                                <li><a href="#home">Anasayfa</a></li>
                                <li><a href="#about">Hakkımda</a></li>
                                <li><a href="#services">Hizmetler</a></li>
                                <li><a href="#contact">İletişim</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /Topbar -->
            <!-- LEFTPART -->
            <div class="arlo_tm_leftpart">
                <div class="inner">
                    <div class="logo">
                        <a class="dark" href="#"><img src="~/mjg/img/logo/logo.png" alt="" /></a>
                        <a class="light" href="#"><img src="~/mjg/img/logo/light.png" alt="" /></a>
                    </div>
                    <div class="menu scrollable">
                        <ul>
                            <li><a href="#home">Anasayfa</a></li>
                            <li><a href="#about">Hakkımda</a></li>
                            <li><a href="#services">Hizmetler</a></li>
                            <li><a href="#contact">İletişim</a></li>
                        </ul>
                    </div>
                    <div class="bottom">
                        <div class="social">
                            <ul>
                                <li><a target="_blank" href="https://www.facebook.com/mert.guney.94801/"><i class="xcon-facebook"></i></a></li>
                                <li><a target="_blank" href="https://twitter.com/mertguney29"><i class="xcon-twitter"></i></a></li>
                                <li><a target="_blank" href="https://www.linkedin.com/in/mert-guney-57745519b/"><i class="xcon-linkedin"></i></a></li>
                                <li><a target="_blank" href="https://www.instagram.com/mertguney29/"><i class="xcon-instagram"></i></a></li>
                                <li><a target="_blank" href="https://github.com/MertGuney"><i class="fa fa-github"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="resize">
                        <a href="#"><span></span></a>
                    </div>
                </div>
            </div>
            <!-- /LEFTPART -->
            <!-- RIGHTPART -->
            <div class="arlo_tm_rightpart">
                <div class="rightpart_inner">

                    <!-- HERO -->
                    <div class="arlo_tm_section" id="home">
                        <div class="arlo_tm_hero_header">
                            <div class="hero_image jarallax" data-img-url="/mjg/img/slider/bg.jpg" data-speed="0"></div>
                            <div id="particles-js"></div>
                            <div class="overlay"></div>
                            <div class="hero_content">
                                <div class="image_wrap">
                                    <img src="~/mjg/img/about/mjg.jpg" alt="" />
                                    <div class="main" data-img-url="/mjg/img/about/mjg.jpg"></div>
                                </div>
                                <div class="name_holder">
                                    <h3>Mert <span>Güney</span></h3>
                                </div>
                                <div class="text_typing">
                                    <p>Ben Bir <span class="arlo_tm_animation_text_word"></span></p>
                                </div>
                            </div>
                            <div class="arlo_tm_arrow_wrap bounce anchor">
                                <a href="#about"><i class="xcon-angle-double-down"></i></a>
                            </div>
                        </div>
                    </div>
                    <!-- /HERO -->
                    <!-- ABOUT -->
                    <div class="arlo_tm_section" id="about">
                        <div class="arlo_tm_about_wrap_all">
                            <div class="arlo_tm_about">
                                <div class="container">
                                    <div class="arlo_title_holder">
                                        <span>Main Info</span>
                                        <h3>Hakkımda</h3>
                                    </div>
                                    <div class="about_inner">
                                        <div class="leftbox">
                                            <div class="about_image_wrap parallax" data-relative-input="true">
                                                <div class="image layer" data-depth="0.1">
                                                    <img src="~/mjg/img/about/550x650.jpg" alt="" />
                                                    <div class="inner" data-img-url="/mjg/img/about/mjg.jpg"></div>
                                                </div>
                                                <div class="border layer" data-depth="0.2">
                                                    <img src="~/mjg/img/about/550x650.jpg" alt="" />
                                                    <div class="inner"></div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="rightbox">
                                            <div class="about_title">
                                                <h3>Ben Mert Güney Ben Bir <span class="arlo_tm_animation_text_word"></span></h3>
                                            </div>
                                            @foreach (var x in Model.Deger1)
                                            {
                                                <div class="text">
                                                    <p>Merhaba Ben <span>Mert Güney</span>. @x.aciklama</p>
                                                </div>
                                                <div class="about_short_contact_wrap">
                                                    <ul>
                                                        <li>
                                                            <img class="svg" src="~/mjg/img/svg/calendar-4.svg" alt="" />
                                                            <span><label>Yaş:</label> @x.yas</span>
                                                        </li>
                                                        <li>
                                                            <img class="svg" src="~/mjg/img/svg/location-1.svg" alt="" />
                                                            <span><label>Bölge:</label> <a class="href_location" href="#">@x.bölge</a></span>
                                                        </li>
                                                        <li>
                                                            <img class="svg" src="~/mjg/img/svg/study-5.svg" alt="" />
                                                            <span><label>Eğitim:</label> <a class="href_location" href="#"> @x.egitim</a></span>
                                                        </li>
                                                        <li>
                                                            <img class="svg" src="~/mjg/img/svg/message-3.svg" alt="" />
                                                            <span><label>Mail:</label> <a href="mailto:@x.mail">@x.mail</a></span>
                                                        </li>
                                                        <li>
                                                            <img class="svg" src="~/mjg/img/svg/call-2.svg" alt="" />
                                                            <span><label>Telefon:</label> <a href="#">@x.telefon</a></span>
                                                        </li>
                                                    </ul>
                                                </div>
                                            }

                                            <div class="buttons">
                                                <ul>
                                                    <li><a href="#"><span>CV Görüntüle</span></a></li>
                                                    <li><a href="#"><span>Mesaj Gönder</span></a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /ABOUT -->
                    <!-- SERVICES -->
                    <div class="arlo_tm_section" id="services">
                        <div class="arlo_tm_services">
                            <div class="container">
                                <div class="arlo_title_holder">
                                    <span>Neler Yapıyorum</span>
                                    <h3>Hizmetlerim</h3>
                                </div>
                                <div class="service_inner">
                                    @foreach (var x in Model.Deger2)
                                    {
                                        <ul>
                                            <li>
                                                <div class="inner">
                                                    <h1><i style="color:black!important" class="svg fa fa-@x.ico"></i></h1>
                                                    <h3>@x.baslik</h3>
                                                    <p>@x.aciklama</p>
                                                </div>
                                            </li>
                                        </ul>
                                    }

                                    <div class="arlo_tm_progress_wrap">
                                        <div class="progress_wrap_inner">
                                            <div class="left">
                                                <div class="main_title">
                                                    <h3>In a <span>short time</span>, I have been able to achieve excellence in all areas of app and web development.</h3>
                                                </div>
                                                <div class="text">
                                                    <p>I provide cost-effective and high quality products to meet our Clients’ needs of timely delivery World Wide. I want to be local in a global world to be where our Customers need our services customers need our services.</p>
                                                </div>
                                            </div>
                                            <div class="right">
                                                <div class="arlo_progress">
                                                    <div class="progress_inner" data-value="80" data-color="#ff214f">
                                                        <span><span class="label">MVC5</span><span class="number">80%</span></span>
                                                        <div class="background"><div class="bar"><div class="bar_in"></div></div></div>
                                                    </div>
                                                    <div class="progress_inner" data-value="70" data-color="#ff214f">
                                                        <span><span class="label">Java / C#</span><span class="number">70%</span></span>
                                                        <div class="background"><div class="bar"><div class="bar_in"></div></div></div>
                                                    </div>
                                                    <div class="progress_inner" data-value="60" data-color="#ff214f">
                                                        <span><span class="label">Javascript</span><span class="number">60%</span></span>
                                                        <div class="background"><div class="bar"><div class="bar_in"></div></div></div>
                                                    </div>
                                                    <div class="progress_inner" data-value="75" data-color="#ff214f">
                                                        <span><span class="label">Unreal Engine 4 / C++</span><span class="number">75%</span></span>
                                                        <div class="background"><div class="bar"><div class="bar_in"></div></div></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /SERVICES -->
                    <!-- TESTIMONIAL -->
                    <div class="arlo_tm_section">
                        <div class="arlo_tm_testimonial">
                            <div class="testimonial_inner">
                                <div class="testimonial_bg">
                                    <div class="image jarallax" data-speed-="0" data-img-url="/mjg/img/slider/1.jpg"></div>
                                    <div class="overlay"></div>
                                </div>
                                <div class="testimonial_content">
                                    <div class="container">
                                        <div class="content_inner">
                                            <div class="icon">
                                                <img class="svg" src="~/mjg/img/svg/quotes.svg" alt="" />
                                            </div>
                                            <div class="texts">
                                                <h5 style="color:deeppink!important"><span>Çalışma Arkadaşlarım</span></h5>
                                                <p>21 yaşında İzmirli bir yazılım mühendisiyim. Freelancer yazılımcı olarak çalışmaktayım. Güncel, profesyonel ve yaratıcı web siteleri yapıyorum ve yönetiyorum. Bunun dışında mobil ve bilgisayar uygulamaları geliştiriyorum.</p>
                                                <div class="author">
                                                    <div class="image">
                                                        <div class="main" data-img-url="/mjg/img/about/fali.png"></div>
                                                    </div>
                                                    <div class="job">
                                                        <h3><a target="_blank" href="https://falidegirmenci.com">Faruk Ali Değirmenci</a></h3>
                                                        <span>Software Developer</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /TESTIMONIAL -->
                    <!-- TIMELINE -->
                    <div class="arlo_tm_section">
                        <div class="arlo_tm_timeline">
                            <div class="container">
                                <div class="arlo_title_holder">
                                    <span>Working Process</span>
                                    <h3>Deneyimlerim</h3>
                                </div>
                                <div class="timeline_inner">
                                    <div class="left">
                                        <ul>
                                            <li>

                                                @foreach (var x in Model.Deger3)
                                                {
                                                    <div class="inner">
                                                        <img class="svg" src="~/mjg/img/svg/degree-5.svg" alt="" />
                                                        <p class="year">@x.egitimTarih</p>
                                                        <h3 class="title">@x.egitimBaslik</h3>
                                                        <p class="text">@x.egitimAciklama</p>
                                                    </div>
                                                }

                                            </li>
                                        </ul>
                                    </div>
                                    <div class="right">
                                        <ul>
                                            <li>

                                                @foreach (var x in Model.Deger3)
                                                {
                                                    <div class="inner">
                                                        <img class="svg" src="~/mjg/img/svg/portfolio.svg" alt="" />
                                                        <p class="year">@x.hizmetTarih</p>
                                                        <h3 class="title">@x.hizmetBaslik</h3>
                                                        <p class="text">@x.hizmetAciklama</p>
                                                    </div>
                                                }

                                            </li>
                                        </ul>
                                    </div>


                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /TIMELINE -->
                    <!-- CONTACT -->
                    <div class="arlo_tm_section" id="contact">
                        <div class="arlo_tm_contact">
                            <div class="container">
                                <div class="arlo_title_holder">
                                    <span>Get in Touch</span>
                                    <h3>İletişim</h3>
                                </div>
                                <div class="contact_inner">
                                    <div class="short_list">
                                        @foreach (var x in Model.Deger1)
                                        {
                                            <ul>
                                                <li>
                                                    <img class="svg" src="~/mjg/img/svg/location-1.svg" alt="" />
                                                    <h3>Bölge</h3>
                                                    <p>@x.bölge</p>
                                                </li>
                                                <li>
                                                    <img class="svg" src="~/mjg/img/svg/message-3.svg" alt="" />
                                                    <h3>Email</h3>
                                                    <p>@x.mail</p>
                                                </li>
                                                <li>
                                                    <img class="svg" src="~/mjg/img/svg/call-2.svg" alt="" />
                                                    <h3>Telefon</h3>
                                                    <p>@x.telefon</p>
                                                </li>
                                            </ul>
                                        }

                                    </div>
                                    <div class="fields">
                                        <form action="https://frenify.com/" method="post" class="contact_form" id="contact_form">
                                            <div class="returnmessage" data-success="Your message has been received, We will contact you soon."></div>
                                            <div class="empty_notice"><span>Please Fill Required Fields</span></div>
                                            <div class="first">
                                                <ul>
                                                    <li>
                                                        <input id="name" type="text" placeholder="Konu">
                                                    </li>
                                                    <li>
                                                        <input id="email" type="text" placeholder="Email">
                                                    </li>
                                                </ul>
                                            </div>
                                            <div class="last">
                                                <textarea id="message" placeholder="Mesaj"></textarea>
                                            </div>
                                            <div class="arlo_tm_button">
                                                <a id="send_message" href="#">Mesaj Gönder</a>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /CONTACT -->
                    <!-- FOOTER -->
                    <div class="arlo_tm_footer">
                        <div class="container">
                            <div class="footer_inner">
                                <div class="copy">
                                    <p>Copyright &copy; 2020.<a href="#" target="_blank">Mert Güney</a></p>
                                </div>
                                <div class="top">
                                    <a class="arlo_tm_totop" href="#"></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /FOOTER -->

                </div>
            </div>
            <!-- /RIGHTPART -->
        </div>
    </div>
    <!-- / WRAPPER ALL -->
    <!-- SCRIPTS -->
    <script src="~/mjg/js/jquery.js"></script>
    <script src="~/mjg/js/plugins.js"></script>
    <script src="~/mjg/js/init.js"></script>
    <!-- /SCRIPTS -->

</body>

</html>